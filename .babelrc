{
  "presets": [
    "react",
    ["env", {
      "useBuiltIns": true,
      "targets": {
        "browsers": [
          "ie >= 11"
        ]
      },
      "modules": false,
      "loose": true,
    }]
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-function-bind",
    "transform-class-properties",
    "syntax-dynamic-import",
    "transform-object-rest-spread", [
      "module-resolver", {
        "root": ["./src/js"]
      }
    ]
  ]
}
